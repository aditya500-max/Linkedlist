/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode() : val(0), next(nullptr) {}
 *     ListNode(int x) : val(x), next(nullptr) {}
 *     ListNode(int x, ListNode *next) : val(x), next(next) {}
 * };
 */
class Solution {
public:
    ListNode* middleNode(ListNode* head) {
        if( head->next==NULL){
      return head;
  }
  ListNode* temp=head;
  int cnt=1;
  while(temp->next!=NULL){
      temp=temp->next;
      cnt++;
  }
  ListNode* t=head;
  ListNode* p=NULL;
  int c=1;
  int mid=cnt/2;
  while(c<=mid){
      p=t;
      t=t->next;
      c++;
  }
 
  return t;
 
        
    }
};
