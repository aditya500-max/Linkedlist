#include<bits/stdc++.h>
using namespace std;
class node{
    public:
    int data;
    node* prev;
    node* next;
    node(int val){
        data=val;
        prev=NULL;
        next=NULL;
    }

};
void deleteAthead(node* &head){
    node* temp=head;
    head=head->next;
    head->prev=NULL;
    delete temp;
}
void deletion(node* &head, int pos){
    if(pos==1){
        deleteAthead(head);
    }
    int cnt=1;
    node* temp=head;
    while(temp!=NULL && pos!=cnt ){
        cnt++;
        temp=temp->next;
    }
    temp->prev->next=temp->next;
    if(temp->next!=NULL){
    temp->next->prev=temp->prev;
    }
    delete temp;
}
void insertHead(node* &head, int val){
       node* n=new node(val);
       n->next=head;
       if(head!=NULL){
       head->prev=n;
       }
       head=n;
}
void insertAttail(node* &head,int val){
    node* n=new node(val);
    if(head==NULL){
        insertHead(head,val);
        return;
    }
    node* temp=head;
    while(temp->next!=NULL){
        temp=temp->next;
    }
    temp->next=n;
    n->prev=temp;
}
void display(node* &head){
    node* temp=head;
    while(temp!=NULL){
         cout<<temp->data<<endl;
         temp=temp->next;
    }
}
int main(){
    node* head=NULL;
    insertAttail(head,1);
    insertAttail(head,4);
    insertAttail(head,3);
    insertAttail(head,9);
    insertHead(head,12);
    insertHead(head,11);
 deleteAthead(head);
    
    display(head);
 
    return 0;
}




















